;
; BIND data file for local loopback interface
;
$TTL	0     ; do not cache, so that we can appreciate
            ; load balancing
@	IN	SOA	rome.exam.edu. root.localhost. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@		IN	NS	rome.exam.edu.
ns	IN	A	192.168.1.5


